pipeline {
    agent any // Specifies that the pipeline can execute on any available agent
    
    stages { // Defines the stages of the pipeline
        stage('build') { // Defines a stage named 'build'
            steps { // Defines the steps to be executed within the 'build' stage
                echo "Hello World!" // Echoes "Hello World!" to the console output
            }
        }
    }
}

